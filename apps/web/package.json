{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "web",
	"private": true,
	"version": "0.0.0",
	"sideEffects": false,
	"type": "module",
	"main": "desktop/index.js",
	"scripts": {
		"build": "cross-env NODE_ENV=production react-router build",
		"check": "tsc",
		"eslint-typegen": "node eslint.config.mjs",
		"deploy": "wrangler pages deploy ./build/client",
		"dev": "react-router dev",
		"electron:dev": "nodemon .",
		"electron:start": "electron .",
		"lint": "eslint --cache . --fix",
		"oxlint": "oxlint -c ./node_modules/oxlint-config/oxlintrc.json --fix",
		"paraglide": "paraglide-js compile --project ./project.inlang --outdir ./app/paraglide",
		"relay": "relay-compiler",
		"relay-watch": "relay-compiler --watch",
		"router": "react-router typegen",
		"start": "vite preview",
		"test": "playwright test"
	},
	"imports": {
		"#lib": "./app/lib"
	},
	"browserslist": [
		"defaults and fully supports es6-module",
		"maintained node versions"
	],
	"dependencies": {
		"@ariakit/react": "0.4.15",
		"@cloudflare/workers-types": "4.20250321.0",
		"@hono/node-server": "1.14.0",
		"@iconify-json/material-symbols": "1.2.17",
		"@inlang/paraglide-js": "2.0.5",
		"@material/material-color-utilities": "0.3.0",
		"@million/lint": "1.0.14",
		"@react-router/dev": "7.4.0",
		"@react-router/node": "7.4.0",
		"@sentry/react": "9.5.0",
		"@sentry/toolbar": "1.0.0-beta.11",
		"@sentry/vite-plugin": "3.2.3",
		"@svgr/core": "8.1.0",
		"@svgr/plugin-jsx": "8.1.0",
		"@tailwindcss/typography": "0.5.16",
		"@tailwindcss/vite": "4.0.0-beta.2",
		"@types/react": "19.0.12",
		"@types/react-dom": "19.0.4",
		"@types/react-relay": "18.2.0",
		"@types/relay-runtime": "18.2.5",
		"anitomy": "0.0.35",
		"arktype": "2.1.15",
		"chokidar": "4.0.3",
		"cookie": "1.0.2",
		"electron": "35.0.1",
		"electron-devtools-installer": "4.0.0",
		"framer-motion": "12.6.2",
		"graphql": "16.10.0",
		"hono": "4.7.5",
		"isbot": "5.1.25",
		"markdown": "workspace:*",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-relay": "18.2.0",
		"react-router": "7.4.0",
		"relay-compiler": "18.2.0",
		"relay-runtime": "18.2.0",
		"tailwind-variants": "npm:@patryk_walach/tailwind-variants@0.2.1",
		"tailwindcss": "4.0.0-beta.2",
		"unplugin-icons": "22.1.0",
		"vite": "6.2.3",
		"vite-env-only": "3.0.3",
		"vite-plugin-babel": "1.3.0",
		"vite-plugin-relay": "2.1.0",
		"vite-tsconfig-paths": "5.1.4"
	},
	"devDependencies": {
		"@graphql-eslint/eslint-plugin": "4.3.0",
		"@playwright/test": "1.51.1",
		"cross-env": "7.0.3",
		"dotenv": "16.4.7",
		"eslint": "9.22.0",
		"eslint-config": "workspace:*",
		"eslint-typegen": "2.1.0",
		"nodemon": "3.1.9",
		"oxlint-config": "workspace:*",
		"ts-config": "workspace:*",
		"vite-plugin-inspect": "11.0.0",
		"vite-plugin-oxlint": "1.3.0"
	},
	"overrides": {
		"react-router-dom": "npm:react-router-dom@7.4.0"
	}
}
