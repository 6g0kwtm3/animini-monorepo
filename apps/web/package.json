{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "web",
	"private": true,
	"version": "0.0.0",
	"sideEffects": false,
	"type": "module",
	"main": "desktop/index.js",
	"scripts": {
		"build": "cross-env NODE_ENV=production react-router build",
		"check": "tsc",
		"eslint-typegen": "node --import=tsx eslint.config.ts",
		"deploy": "wrangler pages deploy ./build/client",
		"dev": "react-router dev",
		"electron:dev": "nodemon .",
		"electron:start": "electron .",
		"lint": "eslint --cache . --fix",
		"oxlint": "oxlint -c ./node_modules/oxlint-config/oxlintrc.json --fix",
		"paraglide": "paraglide-js compile --project ./project.inlang --outdir ./app/paraglide",
		"relay": "relay-compiler",
		"relay-watch": "relay-compiler --watch",
		"router": "react-router typegen",
		"start": "vite preview",
		"test-e2e": "playwright test",
		"update-snapshots": "playwright test --update-snapshots"
	},
	"imports": {
		"#lib": "./app/lib"
	},
	"browserslist": [
		"defaults and fully supports es6-module",
		"maintained node versions"
	],
	"dependencies": {
		"@ariakit/react": "catalog:",
		"@hono/node-server": "catalog:",
		"@iconify-json/material-symbols": "catalog:",
		"@inlang/paraglide-js": "catalog:",
		"@material/material-color-utilities": "catalog:",
		"@react-router/dev": "catalog:",
		"@react-router/node": "catalog:",
		"@sentry/react": "catalog:",
		"@sentry/toolbar": "catalog:",
		"@sentry/vite-plugin": "catalog:",
		"@svgr/core": "catalog:",
		"@svgr/plugin-jsx": "catalog:",
		"@tailwindcss/typography": "catalog:",
		"@tailwindcss/vite": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@types/react-relay": "catalog:",
		"@types/relay-runtime": "catalog:",
		"a": "workspace:*",
		"anitomy": "catalog:",
		"arktype": "catalog:",
		"cookie": "catalog:",
		"electron": "catalog:",
		"electron-devtools-installer": "catalog:",
		"extra-outlet": "workspace:*",
		"framer-motion": "catalog:",
		"graphql": "catalog:",
		"hono": "catalog:",
		"isbot": "catalog:",
		"markdown": "workspace:*",
		"react": "catalog:",
		"react-dom": "catalog:",
		"react-relay": "catalog:",
		"react-router": "catalog:",
		"relay-compiler": "catalog:",
		"relay-runtime": "catalog:",
		"tailwind-variants": "catalog:",
		"tailwindcss": "catalog:",
		"unplugin-icons": "catalog:",
		"vite": "catalog:experimental",
		"vite-plugin-relay": "catalog:",
		"vite-tsconfig-paths": "catalog:"
	},
	"devDependencies": {
		"@babel/preset-typescript": "catalog:",
		"@graphql-eslint/eslint-plugin": "catalog:",
		"@graphql-tools/mock": "catalog:",
		"@msw/playwright": "catalog:",
		"@playwright/test": "catalog:",
		"babel-plugin-react-compiler": "catalog:",
		"cross-env": "catalog:",
		"dotenv": "catalog:",
		"eslint": "catalog:",
		"eslint-config": "workspace:*",
		"eslint-plugin-relay": "workspace:*",
		"eslint-typegen": "catalog:",
		"graphql-config": "catalog:",
		"isbot": "catalog:",
		"msw": "catalog:",
		"nodemon": "catalog:",
		"oxlint-config": "workspace:*",
		"ts-config": "workspace:*",
		"tsx": "catalog:",
		"vite-plugin-babel": "catalog:",
		"vite-plugin-inspect": "catalog:",
		"vite-plugin-oxlint": "catalog:"
	},
	"overrides": {
		"react-router-dom": "npm:react-router-dom@7.6.0"
	}
}
