{
	"$schema": "https://json.schemastore.org/package",
	"name": "markdown",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"exports": {
		".": "./src/index.ts",
		"./Markdown": "./src/markdown.tsx"
	},
	"scripts": {
		"eslint-typegen": "node --import=tsx eslint.config.ts",
		"lint": "eslint --cache . --fix",
		"oxlint": "oxlint -c ./node_modules/oxlint-config/oxlintrc.json --fix",
		"check": "tsc --noEmit"
	},
	"peerDependencies": {
		"@types/react": "19.1.5",
		"react": "19.1.0"
	},
	"dependencies": {
		"@types/marked": "catalog:",
		"dompurify": "catalog:",
		"marked": "catalog:"
	},
	"devDependencies": {
		"eslint": "catalog:",
		"eslint-config": "workspace:*",
		"eslint-typegen": "catalog:",
		"oxlint-config": "workspace:*",
		"ts-config": "workspace:^",
		"tsx": "catalog:"
	}
}
