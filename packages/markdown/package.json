{
	"$schema": "https://json.schemastore.org/package",
	"name": "markdown",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"exports": {
		".": {
			"types": "./tmp/src/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsup src/index.ts --format esm --clean",
		"dev": "tsup src/index.ts --format esm --clean --watch",
		"eslint-typegen": "node eslint.config.mjs",
		"dts": "tsc --noCheck",
		"lint": "eslint --cache . --fix",
		"oxlint": "oxlint -c ./node_modules/oxlint-config/oxlintrc.json --fix",
		"check": "tsc --noEmit"
	},
	"peerDependencies": {
		"@types/react": "19.1.2",
		"react": "19.1.0"
	},
	"dependencies": {
		"@types/marked": "0.3.0",
		"dompurify": "3.2.5",
		"marked": "0.3.19"
	},
	"devDependencies": {
		"eslint": "9.24.0",
		"eslint-config": "workspace:*",
		"eslint-typegen": "2.1.0",
		"oxlint-config": "workspace:*",
		"ts-config": "workspace:^",
		"tsup": "8.4.0"
	}
}
