{
	"$schema": "https://json.schemastore.org/package",
	"name": "eslint-config",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"build": "tsup src/base/eslint.config.ts src/react/eslint.config.ts --format esm --clean",
		"dev": "tsup src/base/eslint.config.ts src/react/eslint.config.ts --format esm --clean --watch",
		"dts": "tsc --noCheck",
		"check": "tsc --noEmit"
	},
	"dependencies": {
		"eslint-plugin-oxlint": "0.16.0",
		"eslint": "9.22.0"
	},
	"devDependencies": {
		"@eslint/js": "9.22.0",
		"eslint-plugin-react": "7.37.4",
		"eslint-plugin-react-compiler": "0.0.0-experimental-fcabbc1-20241106",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.4.19",
		"eslint-plugin-turbo": "2.4.5-canary.4",
		"eslint-typegen": "2.1.0",
		"oxlint-config": "workspace:*",
		"ts-config": "workspace:^",
		"tsup": "8.4.0",
		"typescript-eslint": "8.26.1"
	},
	"exports": {
		"./base": {
			"types": "./tmp/src/base/eslint.config.d.ts",
			"default": "./dist/base/eslint.config.js"
		},
		"./react": {
			"types": "./tmp/src/react/eslint.config.d.ts",
			"default": "./dist/react/eslint.config.js"
		}
	}
}
