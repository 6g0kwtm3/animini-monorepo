{
	"$schema": "https://json.schemastore.org/package",
	"name": "eslint-config",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"devDependencies": {
		"@eslint/js": "9.22.0",
		"@types/eslint": "9.6.1",
		"eslint-plugin-oxlint": "0.15.15",
		"eslint-plugin-react": "7.37.4",
		"eslint-plugin-react-compiler": "0.0.0-experimental-fcabbc1-20241106",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-react-refresh": "0.4.19",
		"eslint-plugin-turbo": "2.4.5-canary.4",
		"eslint-typegen": "2.1.0",
		"oxlint-config": "workspace:*",
		"ts-config": "workspace:^",
		"typescript-eslint": "8.26.1"
	},
	"exports": {
		"./base": {
			"types": "./src/base/eslint-typegen.d.ts",
			"default": "./src/base/eslint.config.mjs"
		},
		"./react": {
			"types": "./src/react/eslint-typegen.d.ts",
			"default": "./src/react/eslint.config.mjs"
		}
	}
}
