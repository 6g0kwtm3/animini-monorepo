{
	"$schema": "https://json.schemastore.org/package.json",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"name": "animini-pwa",
	"scripts": {
		"deploy": "wrangler pages deploy ./build/client",
		"build": "bunx cross-env NODE_ENV=production react-router build",
		"dev": "react-router dev",
		"start": "bun ./scripts/start.ts",
		"prepare": "bun run gen:router && bun run gen:relay && bun run gen:paraglide",
		"gen:relay": "bunx --bun relay-compiler",
		"gen:router": "bunx --bun react-router typegen",
		"gen:paraglide": "paraglide-js compile --project ./project.inlang --outdir=./app/paraglide",
		"check": "bunx --bun tsc",
		"oxlint": "bunx --bun oxlint",
		"lint": "bunx --bun eslint --cache .",
		"format": "bunx --bun prettier . -w",
		"knip": "knip",
		"test:unit": "vitest",
		"test:e2e": "playwright test",
		"dev:storybook": "storybook dev -p 6006 --no-open",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@ariakit/react": "^0.4.15",
		"@material/material-color-utilities": "^0.3.0",
