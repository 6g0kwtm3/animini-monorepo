{
	"private": true,
	"sideEffects": false,
	"type": "module",
	"name": "animini-pwa",
	"main": "desktop/index.js",
	"scripts": {
		"build": "remix vite:build",
		"dev": "pnpm run \"/dev:/\"",
		"dev:vite": "remix vite:dev",
		"dev:codegen": "pnpm run codegen --watch",
		"start": "http-serve ./build/server/index.js",
		"electron:dev": "nodemon .",
		"electron:start": "electron .",
		"check": "tsc",
		"codegen": "graphql-codegen-esm --config codegen.ts",
		"knip": "knip",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"format": "prettier . -w",
		"prepare": "pnpm run codegen",
		"paraglide": "paraglide-js compile --project ./project.inlang --outdir=./app/paraglide",
		"test-ct": "playwright test -c playwright-ct.config.ts"
	},
	"dependencies": {
		"@ariakit/react": "^0.4.1",
