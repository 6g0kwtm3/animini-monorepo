		"@remix-run/react": "^2.7.1",
		"anitomy": "^0.0.34",
		"chokidar": "^3.6.0",
		"cookie": "^0.6.0",
		"dompurify": "^3.0.8",
		"effect": "^2.2.5",
		"framer-motion": "^11.0.3",
