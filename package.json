{
	"$schema": "https://json.schemastore.org/package.json",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"name": "animini-pwa",
	"main": "desktop/index.js",
	"scripts": {
		"deploy": "wrangler pages deploy ./build/client",
		"build": "bun run paraglide && bunx cross-env NODE_ENV=production react-router build",
		"dev": "react-router dev",
		"dev:codegen": "bun run codegen",
		"start": "vite preview",
		"electron:dev": "nodemon .",
		"electron:start": "electron .",
		"check": "bunx --bun tsc",
		"codegen": "relay-compiler",
		"knip": "knip",
		"oxlint": "oxlint -c ./oxlintrc.json",
		"lint": "bunx --bun eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"format": "bunx --bun prettier . -w",
		"prepare": "bun run codegen",
		"paraglide": "paraglide-js compile --project ./project.inlang --outdir=./app/paraglide",
		"test": "playwright test"
	},
	"dependencies": {
		"@ariakit/react": "^0.4.15",
		"@material/material-color-utilities": "^0.3.0",
		"@million/lint": "1.0.0-rc.26",
		"@react-router/node": "^7.1.3",
		"@sentry/react": "^8.50.0",
		"anitomy": "^0.0.35",
		"arktype": "^2.0.0",
		"chokidar": "^3.6.0",
		"cookie": "^0.6.0",
		"dompurify": "^3.2.3",
		"framer-motion": "^11.18.2",
		"isbot": "^4.4.0",
		"marked": "^0.3.19",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-relay": "^16.2.0",
		"react-router": "^7.1.3",
		"relay-runtime": "^16.2.0",
		"tailwind-variants": "npm:@patryk_walach/tailwind-variants@^0.1.18"
	},
	"overrides": {
		"react-router-dom": "npm:react-router-dom@^7.1.1"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "4.20230914.0",
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@graphql-eslint/eslint-plugin": "^4.3.0",
		"@iconify-json/material-symbols": "^1.2.12",
		"@inlang/paraglide-js": "2.0.0-beta.12",
		"@inlang/paraglide-vite": "^1.3.5",
		"@playwright/test": "1.45.3",
		"@react-router/dev": "^7.1.3",
		"@sentry/vite-plugin": "^3.0.0",
		"@svgr/core": "^8.1.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/typography": "^0.5.16",
		"@types/cookie": "^0.6.0",
		"@types/dompurify": "^3.2.0",
		"@types/eslint": "^9.6.1",
		"@types/marked": "~0.3.0",
		"@types/react": "^19.0.7",
		"@types/react-dom": "^19.0.3",
		"@types/react-relay": "^16.0.6",
		"@types/relay-runtime": "^14.1.24",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.7",
		"electron": "^28.3.3",
		"electron-devtools-installer": "^3.2.1",
		"eslint": "9.12.0",
		"eslint-plugin-compat": "6.0.1",
		"eslint-plugin-oxlint": "^0.15.7",
		"eslint-plugin-react-compiler": "0.0.0-experimental-fcabbc1-20241106",
		"eslint-plugin-react-refresh": "0.4.12",
		"graphql": "^16.10.0",
		"knip": "^5.43.1",
		"million": "^3.1.11",
		"oxlint": "^0.15.7",
		"prettier": "^3.4.2",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"relay-compiler": "^16.2.0",
		"tailwindcss": "^3.4.17",
		"typescript": "5.7.3",
		"typescript-eslint": "^8.21.0",
		"unplugin-icons": "^0.22.0",
		"vite": "^6.0.11",
		"vite-env-only": "^3.0.3",
		"vite-plugin-babel": "^1.3.0",
		"vite-plugin-oxlint": "^1.2.1",
		"vite-plugin-relay": "^2.1.0",
		"vite-tsconfig-paths": "^4.3.2",
		"wrangler": "3.34.2"
	},
	"engines": {
		"node": ">=23.6.0"
	},
	"packageManager": "bun@1.2.0",
