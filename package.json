{
	"$schema": "https://json.schemastore.org/package.json",
	"private": true,
	"scripts": {
		"knip": "knip",
		"oxlint": "oxlint -c ./oxlintrc.json",
		"lint": "bunx --bun eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"format": "bunx --bun prettier . -w",
		"check": "bun --filter * check"
	},
	"devDependencies": {
		"@playwright/test": "1.51.0",
		"@eslint/eslintrc": "3.3.0",
		"@eslint/js": "9.22.0",
		"@graphql-eslint/eslint-plugin": "4.3.0",
		"@types/eslint": "9.6.1",
		"eslint": "9.22.0",
		"eslint-plugin-compat": "6.0.2",
		"eslint-plugin-oxlint": "0.15.14",
		"eslint-plugin-react-compiler": "0.0.0-experimental-fcabbc1-20241106",
		"eslint-plugin-react-refresh": "0.4.19",
		"knip": "5.45.0",
		"oxlint": "0.15.14",
		"prettier": "3.5.3",
		"prettier-plugin-tailwindcss": "0.6.11",
		"typescript": "5.8.2",
		"typescript-eslint": "8.26.1",
		"vite-plugin-babel": "1.3.0",
		"vite-plugin-inspect": "11.0.0",
		"vite-plugin-oxlint": "1.2.3",
		"wrangler": "3.114.1"
	},
	"engines": {
		"node": ">=23.6.0"
	},
	"packageManager": "bun@1.2.0",
	"workspaces": [
		"packages/*"
	]
}
