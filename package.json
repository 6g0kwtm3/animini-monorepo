		"@graphql-typed-document-node/core": "^3.2.0",
		"@iconify-json/material-symbols": "^1.1.73",
		"@inlang/paraglide-js": "^1.2.3",
		"@inlang/paraglide-js-adapter-vite": "^1.2.12",
