		"@types/sanitize-html": "^2.11.0",
		"@vercel/remix": "^2.8.0",
		"autoprefixer": "^10.4.17",
		"electron": "^28.2.2",
		"electron-devtools-installer": "^3.2.0",
		"eslint": "^8.38.0",
		"eslint-plugin-compat": "^4.2.0",
