		"anitomy": "^0.0.34",
		"chokidar": "^3.6.0",
		"cookie": "^0.6.0",
		"effect": "^2.2.5",
		"framer-motion": "^11.0.3",
		"htmlparser2": "^9.1.0",
		"isbot": "^4.1.0",
		"marked": "^0.3.19",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"remix-utils": "^7.5.0",
		"sanitize-html": "^2.11.0",
		"tailwind-variants": "npm:@patryk_walach/tailwind-variants@^0.1.18"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.2",
