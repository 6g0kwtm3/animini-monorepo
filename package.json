{
	"private": true,
	"sideEffects": false,
	"type": "module",
	"name": "animini-pwa",
	"main": "desktop/index.js",
	"scripts": {
		"deploy": "wrangler pages deploy ./build/client",
		"build": "remix vite:build",
		"dev": "remix vite:dev",
		"start": "wrangler pages dev ./build/client",
		"dev:electron": "nodemon .",
		"start:electron": "electron .",
		"check": "tsc",
		"codegen": "graphql-codegen-esm --config codegen.ts",
		"knip": "knip",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"format": "prettier . -w",
		"prepare": "pnpm run codegen && paraglide-js compile --project ./project.inlang --outdir=./app/paraglide && wrangler types"
	},
