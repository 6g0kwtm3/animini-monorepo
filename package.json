		"react-dom": "^19.0.0",
		"react-relay": "^18.2.0",
		"react-router": "^7.1.1",
		"relay-runtime": "^18.2.0",
		"tailwind-variants": "npm:@patryk_walach/tailwind-variants@^0.2.0"
	},
