	"name": "animini-pwa",
	"main": "desktop/index.js",
	"scripts": {
		"deploy": "wrangler pages deploy ./build/client",
		"build": "remix vite:build",
		"dev": "pnpm run \"/dev:/\"",
		"dev:vite": "remix vite:dev",
		"dev:codegen": "pnpm run codegen",
		"start": "wrangler pages dev ./build/client --port 3000",
		"electron:dev": "nodemon .",
		"electron:start": "electron .",
		"check": "tsc",
		"codegen": "relay-compiler",
		"knip": "knip",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"format": "prettier . -w",
		"prepare": "wrangler types && pnpm run codegen",
		"paraglide": "paraglide-js compile --project ./project.inlang --outdir=./app/paraglide",
		"test": "playwright test"
	},
