{
	"$schema": "https://json.schemastore.org/package.json",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"name": "animini-pwa",
	"main": "desktop/index.js",
	"scripts": {
		"deploy": "wrangler pages deploy ./build/client",
		"build": "bunx cross-env NODE_ENV=production react-router build",
		"dev": "react-router dev",
		"dev:codegen": "bun run codegen",
		"start": "vite preview",
		"electron:dev": "nodemon .",
		"electron:start": "electron .",
		"check": "bunx --bun tsc",
		"codegen": "relay-compiler",
		"gen:router": "bunx --bun react-router typegen",
		"knip": "knip",
		"oxlint": "oxlint -c ./oxlintrc.json",
		"lint": "bunx --bun eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"format": "bunx --bun prettier . -w",
		"prepare": "bun run paraglide && bun run gen:router && bun run codegen",
		"paraglide": "paraglide-js compile --project ./project.inlang --outdir ./app/paraglide",
		"test": "playwright test"
	},
	"dependencies": {
		"@ariakit/react": "^0.4.15",
		"@material/material-color-utilities": "^0.3.0",
		"@million/lint": "1.0.14",
		"@react-router/node": "^7.3.0",
		"@sentry/react": "^9.5.0",
		"@sentry/toolbar": "^1.0.0-beta.11",
		"anitomy": "^0.0.35",
		"arktype": "^2.0.0",
		"chokidar": "^3.6.0",
		"cookie": "^0.6.0",
		"dompurify": "^3.2.3",
		"framer-motion": "^11.18.2",
		"isbot": "^4.4.0",
		"marked": "^0.3.19",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-relay": "^18.2.0",
		"react-router": "^7.3.0",
		"relay-runtime": "^18.2.0",
		"tailwind-variants": "npm:@patryk_walach/tailwind-variants@^0.2.1"
	},
	"overrides": {
		"react-router-dom": "npm:react-router-dom@^7.3.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "4.20250303.0",
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@graphql-eslint/eslint-plugin": "^4.3.0",
		"@iconify-json/material-symbols": "^1.2.12",
		"@inlang/paraglide-js": "^1.11.8",
		"@inlang/paraglide-vite": "^1.3.5",
		"@playwright/test": "1.51.0",
		"@react-router/dev": "^7.3.0",
		"@sentry/vite-plugin": "^3.2.2",
		"@svgr/core": "^8.1.0",
		"@svgr/plugin-jsx": "^8.1.0",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "4.0.0-beta.2",
		"@types/cookie": "^0.6.0",
		"@types/dompurify": "^3.2.0",
		"@types/eslint": "^9.6.1",
		"@types/marked": "~6.0.0",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@types/react-relay": "^18.2.0",
		"@types/relay-runtime": "^18.2.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.7",
		"electron": "^28.3.3",
		"electron-devtools-installer": "^3.2.1",
		"eslint": "9.22.0",
		"eslint-plugin-compat": "6.0.2",
		"eslint-plugin-oxlint": "^0.15.13",
